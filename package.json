{
  "name": "quickstart-lib",
  "version": "1.0.1",
  "description": "Library QuickStart package.json from the documentation, supplemented with testing support",
  "main": "./bundles/quickstart-lib.umd.js",
  "module": "./quickstart-lib.es5.js",
  "es2015": "./quickstart-lib.js",
  "typings": "./quickstart-lib.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rimraf out-tsc dist/* dist-demo/*",
    "start": "ng serve",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "prebuild-demo": "npm run clean",
    "build-demo": "ng build -aot -bo -scd --base-href \"https://jgodi.github.io/angular-quickstart-lib/\"",
    "lint": "tslint ./src/**/*.ts -t verbose --type-check --project tsconfig.json",
    "release": "standard-version",
    "postrelease": "npm run gh-pages",
    "pregh-pages": "npm run build-demo",
    "gh-pages": "gh-pages -d dist-demo",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "ng e2e"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.ts",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "jest-global-mocks.ts",
      "jest.ts"
    ]
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0 <5.0.0 || >=4.0.0-beta <5.0.0"
  },
  "devDependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/cli": "1.3.0-rc.3",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^20.0.6",
    "@types/node": "~6.0.60",
    "camelcase": "^4.0.0",
    "codelyzer": "~3.1.1",
    "core-js": "^2.4.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^20.0.4",
    "jest-preset-angular": "^2.0.5",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.42.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "standard-version": "^4.0.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "zone.js": "^0.8.14"
  }
}
